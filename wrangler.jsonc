{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "romaingrx",
  "main": "./dist/_worker.js/index.js",
  "compatibility_date": "2025-07-25",
  "compatibility_flags": ["nodejs_compat", "global_fetch_strictly_public"],
  "assets": {
    "binding": "ASSETS",
    "directory": "./dist",
  },
  "observability": {
    "enabled": true,
  },
  "routes": [
    {
      "pattern": "romaingrx.com",
      "custom_domain": true,
    },
  ],
}
